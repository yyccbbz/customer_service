<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<excels>
	<!-- excel标签参看:ExcelDefinition,Field标签参看:FieldValue -->

	<!-- 客诉客户投资明细 -->
	<excel id="csUser" class="com.evergrande.customer_service.model.CsUser"
		   sheetname="客诉客户投资明细" defaultColumnWidth="5000" enableStyle="true" defaultAlign="center">
		<field name="id" title="id" columnWidth="2000"/>
		<field name="phoneNumber" title="手机号码" columnWidth="4000"/>
		<field name="productName" title="产品名称" columnWidth="7000"/>
		<field name="settlementCapital" title="投资金额" columnWidth="4000"/>
		<field name="inceptionDate" title="投资起始时间"/>
		<field name="dueDate" title="投资到期时间"/>
		<field name="idNo" title="证件号码"/>
		<field name="userName" title="客户姓名" columnWidth="3000"/>
		<field name="mtime" title="更新时间" pattern="yyyy-MM-dd HH:mm:ss"/>
		<field name="ctime" title="创建时间" pattern="yyyy-MM-dd HH:mm:ss"/>
	</excel>

</excels>
